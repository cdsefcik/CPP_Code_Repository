#include<iostream>
using namespace std;



int main(){

    int e = {8};
    int c {5};
    int a[10];
    a[0] = 2;
    a[4] = 5;

    int b[4] = {1,2,3,4};

    cout<<a[0]<<" "<<a[4]<<endl;
    cout<<b[0]<<b[1]<<b[2]<<b[3]<<endl;

    for(int i = 0; i < 10; i++){
        cout << a[i]<<" ";
    }
    cout<<sizeof(b[4]);
    return 0;
}
